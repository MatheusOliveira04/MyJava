		Scanner ler = new Scanner(System.in);

		System.out.println("Entre os dados do contrato:");
		System.out.print("Numero: ");
		int number = ler.nextInt();
		System.out.print("Data (dd/MM/yyyy):");
		
		ler.close();
	}

}
